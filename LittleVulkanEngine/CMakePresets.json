{
  "version": 3,
  "cmakeMinimumRequired": {
    "major": 3, "minor": 19, "patch": 0
  },
  "configurePresets": [
    {
      "name": "linux-clang-debug",
      "generator": "Ninja",
      "binaryDir": "${sourceDir}/build",
      "cacheVariables": {
        "CMAKE_BUILD_TYPE": "Debug",
        "CMAKE_C_COMPILER": "clang",
        "CMAKE_CXX_COMPILER": "clang++"
      }
    }
  ],
  "buildPresets": [
    {
      "name": "linux-clang-debug",
      "configurePreset": "linux-clang-debug"
    }
  ],
  "testPresets": []
}